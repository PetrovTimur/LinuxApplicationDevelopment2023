#: guess.c:16
msgid "Gueeser -- a minimal program that guesses your number between 1 and 100"
msgstr "Угадайка -- минимальная программа, угадывающая ваше число от 1 до 100"

#: guess.c:19
msgid "Use roman numerals"
msgstr "Использовать римские цифры"

#: guess.c:92
#, c-format
msgid "Pick a number between %s and %s\n"
msgstr "Загадайте число от %s до %s\n"

#: guess.c:99
#, c-format
msgid "Is your number more than %s? [yes/no]\t\t"
msgstr "Ваше число больше чем %s? [да/нет]\t\t"

#: guess.c:105
msgid "yes"
msgstr "да"

#: guess.c:107
msgid "no"
msgstr "нет"

#: guess.c:114
#, c-format
msgid "Your number is: %s\n"
msgstr "Ваше число: %s\n"
